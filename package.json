    "jwt-decode": "^2.2.0",
    "mongoose": "^5.4.18",
    "node-sass": "^4.11.0",
    "oauth": "^0.9.15",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.8.4",
